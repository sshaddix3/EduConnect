{
  "name": "InstantTutor",
  "description": "Get help on anything, anytime!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["http://localhost:3001/"],
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "permissions": ["tabs", "activeTab", "identity", "storage", "cookies"]
}
